package random_progs;

import java.util.ArrayList;

public class recur {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static ArrayList<Integer> x(int[] a,int t,int i, ArrayList<Integer> list){
		if(i==a.length) {
			return list;
		}
		if(i!=a.length && a[i]==t) {
			list.add(i);
		}
		return x(a,t,++i,list);
		
	}
}
